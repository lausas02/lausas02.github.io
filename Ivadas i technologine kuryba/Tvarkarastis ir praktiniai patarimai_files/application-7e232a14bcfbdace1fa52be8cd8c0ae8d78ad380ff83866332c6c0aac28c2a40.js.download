const sendAjaxPost = (url, type, data, success, error) => {
    $.ajaxSetup({
        headers: {
            'X-CSRF-Token': $('meta[name="csrf-token"]').attr('content')
        }
    });

    $.ajax({
        url: url,
        type: type,
        data: JSON.stringify(data),
        dataType: 'json',
        contentType: "application/json; charset=utf-8",
        async: true,
        success: function () {
            if (success)
                success()
        },
        error: function (data) {
            if (error)
                error()
        }
    });
};
